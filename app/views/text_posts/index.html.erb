<div>
  <% @text_posts.each do |textpost| %>
    <%= div_for textpost do %>
      <%= content_tag :p, textpost.title %>
      <p>
        <%= link_to "Show", textpost %>
      </p>
    <% end %>
  <% end %>
  <nav>
    <ul>
      <li><%= link_to "Home", root_path %></li>
      <li><%= link_to "New TextPost", new_text_post_path %></li>
    </ul>
  </nav>
</div>